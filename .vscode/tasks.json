{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: clang build processor",
      "command": "/usr/bin/g++-12",
      "args": [
        "-g",
        "processor/main.cpp",
        "solver/*.cpp",
        "-o",
        "processor/haversine_processor",
        "-I",
        "processor",
        "-I",
        ".",
        "-std=c++17"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": ["$gcc"],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger."
    },
    {
      "type": "cppbuild",
      "label": "C/C++: clang test processor",
      "command": "/usr/bin/clang++",
      "args": [
        "-fcolor-diagnostics",
        "-fansi-escape-codes",
        "-g",
        "processor/test.cpp",
        "-o",
        "processor/test",
        "-I",
        "processor"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": ["$gcc"],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger."
    },
    {
      "type": "cppbuild",
      "label": "C/C++: clang build generator",
      "command": "/usr/bin/clang++",
      "args": [
        "-fcolor-diagnostics",
        "-fansi-escape-codes",
        "-g",
        "generator/*.cpp",
        "solver/*.cpp",
        "-o",
        "generator/haversine_generator",
        "-I",
        "generator",
        "-I",
        "."
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": ["$gcc"],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger."
    }
  ],
  "version": "2.0.0"
}
